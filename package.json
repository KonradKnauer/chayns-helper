    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.4",
    "node-sass": "^4.11.0",
    "postcss-loader": "^4.0.1",
    "prop-types": "^15.7.2",
    "rollup": "^2.26.11",
    "rollup-plugin-auto-external": "^2.0.0",
