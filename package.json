{
  "name": "chayns-helper",
  "version": "2.1.4",
  "author": "Christian Nagel",
  "description": "my personal helpers for chayns development",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/chayns-helper.esm.js",
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "tsdx build",
    "install:all": "npm i -reg http://repo.tobit.ag:8081/repository/npm/ && npm i chayns-doc@latest -D",
    "pub": "npm publish",
    "pub:beta": "npm version prerelease --preid=beta -m \":bookmark: Prerelease version %s\" && npm publish && deprecate.bat \"Version #pkgversion# is unstable, please install a stable version\"",
    "pub:patch": "npm version patch -m \":bookmark: Patch v%s\" && npm publish",
    "pub:patch:deprecate": "deprecate.bat \"Version #pkgversion# contains errors, please update to @latest\" && npm version patch -m \":bookmark: Patch v%s\" && npm publish",
    "pub:minor": "npm version minor -m \":bookmark: Minor release v%s\" && npm publish",
    "pub:major": "npm version major -m \":bookmark: Major release v%s\" && npm publish",
    "prepublishOnly": "npm run build",
    "preversion": "npm run install:all",
    "postpublish": "git push && git push --tags",
    "createLink": "npm link",
    "removeLink": "npm unlink"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "react-redux": ">=7.1.3",
    "react": ">=16",
    "chayns-logger": ">=2.8.5",
    "chayns-components": ">=4.20.4"
  },
  "peerDependenciesMeta": {
    "react-redux": {
      "optional": true
    },
    "react": {
      "optional": false
    },
    "chayns-logger": {
      "optional": false
    },
    "chayns-components": {
      "optional": false
    }
  },
  "devDependencies": {
    "@babel/plugin-proposal-logical-assignment-operators": "^7.8.3",
    "@babel/plugin-proposal-numeric-separator": "^7.8.3",
    "@babel/plugin-proposal-pipeline-operator": "^7.8.3",
    "@babel/plugin-proposal-throw-expressions": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "autoprefixer": "^9.7.4",
    "babel-plugin-const-enum": "^1.0.1",
    "babel-plugin-optimize-clsx": "^2.6.1",
    "babel-plugin-pure-typescript-enum": "^0.1.1",
    "babel-plugin-transform-remove-imports": "^1.4.1",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "babel-plugin-typescript-iife-enum": "^0.2.1",
    "chayns-components": "^4.20.6",
    "chayns-doc": "^1.1.9",
    "chayns-logger": "^2.8.6",
    "cssnano": "^4.1.10",
    "eslint": "^7.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.4",
    "jsdoc": "^3.6.6",
    "node-sass": "^4.11.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.1.3",
    "rollup-plugin-postcss": "^3.1.4",
    "rollup-plugin-scss": "^2.6.1",
    "tsc": "^1.20150623.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "abortcontroller-polyfill": "^1.5.0",
    "clsx": "^1.1.1",
    "date-fns": "^2.16.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chincoe/chayns-helper.git"
  },
  "keywords": [
    "chayns",
    "react",
    "hooks"
  ],
  "bugs": {
    "url": "https://github.com/chincoe/chayns-helper/issues"
  },
  "homepage": "https://github.com/chincoe/chayns-helper#readme"
}
